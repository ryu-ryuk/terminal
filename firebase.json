{
    "hosting": {
        "site": "archya",
        "public": "dist",
        "cleanUrls": true,
        "rewrites": [
            {
                "source": "/about",
                "destination": "/about.html"
            },
            {
                "source": "/projects",
                "destination": "/projects.html"
            },
            {
                "source": "/exp",
                "destination": "/exp.html"
            },
            {
                "source": "/skills",
                "destination": "/skills.html"
            },
            {
                "source": "/contact",
                "destination": "/contact.html"
            }
        ]
    },
    "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
    ]
}